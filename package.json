{
  "name" : "pinecone-server",
  "version" : "1.1.0",
  "description" : "The backend for pinceone",
  "main" : "app.js",
  "bugs" : {
    "url" : "https://gitlab.com/ReallyBigTree/Pinecone-Server/issues",
    "email" : "jim@reallybigtree.com"
  },
  "scripts" : {
    "pinecone:server:docker:build" : "docker build --build-arg NPM_TOKEN=6f3bcf35-125d-40da-bfab-dcfb6efece79 -t registry.gitlab.com/reallybigtree/pinecone-server .",
    "pinecone:server:docker:push" : "docker push registry.gitlab.com/reallybigtree/pinecone-server",
    "pinecone:server:docker:run:dev" : "docker run --detach --name pinecone-server -p 3000:3000 --label=traefik.backend=api --label=traefik.frontend.rule=Host:localhost/api --label=traefik.port=3000 -v $(pwd):/usr/src/app --link pinecone-mongo:mongo --link pinecone-rabbitmq:rabbitmq --link pinecone-redis:redis registry.gitlab.com/reallybigtree/pinecone-server",
    "pinecone:server:docker:run:staging" : "docker run --detach --name pinecone-server registry.gitlab.com/reallybigtree/pinecone-server",
    "pinecone:server:start" : "nodemon --exec ./node_modules/.bin/babel-node --presets es2015,stage-2 ./bin/www",
    "pinecone:server:test:e2e" : "mocha -t 100000 --compilers js:babel-core/register",
    "pinecone:server:test:unit:single-run" : "karma start karma.conf.js --single-run",
    "pinecone:server:update-webdriver" : "webdriver-manager update",
    "prepinecone:destinations:twitter:docker:build" : "npm install;npm prune",
    "start" : "nodemon --exec ./node_modules/.bin/babel-node --presets es2015,stage-2 ./bin/www"
  },
  "author" : {
    "name" : "Jim Barrows",
    "email" : "jim@reallybigtree.com"
  },
  "contributors" : [],
  "license" : "UNLICENSED",
  "private" : true,
  "repository" : {
    "type" : "git",
    "url" : "https://gitlab.com/ReallyBigTree/Pinecone-Server/tree/master"
  },
  "dependencies" : {
    "@reallybigtree/pinecone-models" : "^1.3.0",
    "amqplib" : "^0.4.2",
    "babel-cli" : "^6.10.1",
    "babel-core" : "^6.10.4",
    "babel-preset-es2015" : "^6.9.0",
    "babel-preset-stage-2" : "^6.11.0",
    "bluebird" : "^3.4.1",
    "body-parser" : "~1.15.1",
    "connect-redis" : "^3.1.0",
    "cookie-parser" : "~1.4.3",
    "debug" : "~2.2.0",
    "express" : "~4.13.4",
    "express-session" : "^1.14.1",
    "less-middleware" : "1.0.x",
    "moment" : "^2.14.1",
    "mongoose" : "^4.5.3",
    "morgan" : "~1.7.0",
    "nodemon" : "^1.9.2",
    "oauth-1.0a" : "^1.0.1",
    "passport" : "^0.3.2",
    "passport-facebook" : "^2.1.1",
    "passport-local" : "^1.0.0",
    "passport-local-mongoose" : "^4.0.0",
    "pug" : "^2.0.0-beta5",
    "rabbit.js" : "^0.4.4",
    "redis" : "^2.6.2",
    "serve-favicon" : "~2.3.0",
    "twitter" : "^1.4.0"
  },
  "devDependencies" : {
    "axios" : "^0.12.0",
    "babel-polyfill" : "^6.13.0",
    "chai" : "^3.5.0",
    "mocha" : "^3.0.2",
    "babel-cli" : "^6.10.1",
    "babel-core" : "^6.10.4",
    "babel-preset-es2015" : "^6.9.0",
    "babel-preset-stage-2" : "^6.11.0"
  }
}
