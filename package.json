    "rabbit.js" : "^0.4.4",
    "serve-favicon" : "~2.3.0",
    "twitter" : "^1.4.0"
  },
  "devDependencies" : {
    "axios" : "^0.12.0",
    "chai" : "^3.5.0",
    "jasmine" : "^2.4.1",
