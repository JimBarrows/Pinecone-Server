{
  "name": "pinecone-server",
  "version": "1.1.0",
  "description": "The backend for pinceone",
  "main": "app.js",
  "bugs": {
    "url": "https://github.com/JimBarrows/Pinecone-Server/issues"
  },
  "scripts": {
    "docker:push": "docker push registry.gitlab.com/reallybigtree/pinecone-Server",
    "docker:build": "docker build -t registry.gitlab.com/reallybigtree/pinecone-Server .",
    "docker:run": "docker run --detach --name pinecone-Server --label=traefik.backend=Server --label=traefik.port=8080 --link pinecone-server:server registry.gitlab.com/reallybigtree/pinecone-Server",
    "package": "webpack",
    "start": "webpack-dev-server --hot --inline",
    "test": "wdio wdio.conf.js",
    "test:webdriver:start": "webdriver-manager start",
    "test:update-webdriver": "webdriver-manager update",
    "predocker:build": "npm run pinecone:Server:package"
  },
  "author": {
    "name": "Jim Barrows",
    "email": "Jim.Barrows@gmail.com"
  },
  "contributors": [],
  "license": "Apache 2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:JimBarrows/Pinecone-Server.git"
  },
  "dependencies": {
    "@reallybigtree/pinecone-models": "^1.0.0",
    "amqplib": "^0.4.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "bluebird": "^3.4.1",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.13.0",
    "less-middleware": "1.0.x",
    "moment": "^2.14.1",
    "mongoose": "^4.5.3",
    "morgan": "~1.7.0",
    "nodemon": "^1.9.2",
    "oauth-1.0a": "^1.0.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "pug": "^2.0.0-beta5",
    "rabbit.js": "^0.4.4",
    "serve-favicon": "~2.3.0",
    "twitter": "^1.4.0"
  },
